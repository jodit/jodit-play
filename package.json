{
  "name": "jodit-master",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jodit": "^3.1.36",
    "jodit-react": "^1.0.14",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "2.0.0-next.47d2d941",
    "react-syntax-highlighter": "^7.0.0",
    "react-toggle": "^4.0.2"
  },
  "scripts": {
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --regex --find \"main\\.[^\\.]+\\.js(\\.map)?\" --replace \"main.js$1\" build\\static\\js\\*",
    "build-rename-css": "renamer --regex --find \"main\\.[^\\.]+\\.css(\\.map)?\"  --replace \"main.css$1\" build\\static\\css\\*",
    "start": "react-scripts start",
    "build": "react-scripts build && npm run build-rename",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": "https://xdsoft.net/jodit/assets/",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "devDependencies": {
    "renamer": "^0.6.1"
  }
}
