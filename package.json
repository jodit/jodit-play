{
  "name": "jodit-play",
  "version": "0.1.37",
  "dependencies": {
    "history": "^4.7.2",
    "react-color": "^2.14.0",
    "tinycolor2": "^1.4.1"
  },
  "scripts": {
    "doc": "typedoc --mode modules  --module umd --json ./public/data.json --ignoreCompilerErrors --tsconfig ./node_modules/jodit/tsconfig.json ./node_modules/jodit/src/",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --regex --find \"main\\.[^\\.]+\\.js(\\.map)?\" --replace \"main.js$1\" build\\static\\js\\*",
    "build-rename-css": "renamer --regex --find \"main\\.[^\\.]+\\.css(\\.map)?\"  --replace \"main.css$1\" build\\static\\css\\*",
    "newversion": "npm run build && npm version patch --no-git-tag-version && npm run github && npm publish ./",
    "github": "git add --all  && git commit -m \"New version %npm_package_version%. Read more https://github.com/xdan/jodit/releases/tag/%npm_package_version% \" && git tag %npm_package_version% && git push --tags origin HEAD:master",
    "start": "react-app-rewired  start",
    "build": "npm run doc && react-app-rewired  build && npm run build-rename",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "homepage": "/jodit/node_modules/jodit-play/build/",
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "devDependencies": {
    "jodit": "^3.1.82",
    "jodit-react": "^1.0.14",
    "react": "^16.3.0",
    "react-app-rewired": "^1.5.0",
    "react-dom": "^16.3.0",
    "react-scripts": "2.0.0-next.47d2d941",
    "react-syntax-highlighter": "^7.0.2",
    "react-toggle": "^4.0.2",
    "renamer": "^0.6.1",
    "typedoc": "^0.10.0"
  }
}
